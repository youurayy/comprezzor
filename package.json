{
    "author": "Juraj Vitko (@ypocat)",
    "name": "comprezzor",
    "description": "easy and reusable way to compress and decompress text data without headers (e.g. for storage in redis)",
    "keywords": [ "zlib", "deflate", "inflate", "compress", "compression" ],
    "version": "0.0.5",
    "homepage": "http://github.com/ypocat/comprezzor",
    "repository": {
        "type": "git",
        "url": "git://github.com/ypocat/comprezzor.git"
    },
    "main": "lib/comprezzor.js",
    "directories": {
        "lib": "./lib"
    },
    "engines": {
        "node": "*"
    },
    "dependencies": {
        "laeh2": "*",
        "clone": "*"
    },
    "devDependencies": {
        "mocha": "*"
    },
    "scripts": {
        "test": "mocha -t 10000 -s 2000 -R list",
        "test-dev": "mocha -t 10000 -s 2000 -R list -w"
    },
    "licenses": [
